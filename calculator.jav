class MyCalculator {
    int sum(int x, int y) {
            return x + y;
         }
        
    int subtract(int x, int y) {
            return x - y;
        } 
    
    int multiply(int x, int y) {
            return x * y;
        }
    
    int divid(int x, int y) {
            return x / y;
    }
}